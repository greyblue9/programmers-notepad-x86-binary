<!--
Utilizes "Solarized" color scheme created by Ethan Schoonover.
More info at ethanschoonover.com/solarized

base03    #002b36
base02    #073642
base01    #586e75
base00    #657b83
base0     #839496
base1     #93a1a1
base2     #eee8d5
base3     #fdf6e3
yellow    #b58900
orange    #cb4b16
red       #dc322f
magenta   #d33682
violet    #6c71c4
blue      #268bd2
cyan      #2aa198
green     #859900
-->
<UserSettings>
	<override-colours caret="dc322f" indentGuides="93a1a1" markAll="dc322f" selBack="93a1a1" selFore="eee8d5" smartHighlight="cd4b16" templateField="268bd2" />
	<override-classes>
		<style-class name="default" 		fore="657b83" back="fdf6e3" font="Anonymous Pro" />
		<style-class name="attribute" 		fore="002b36" bold="true" />
		<style-class name="bracematch" 		fore="fdf6e3" back="839496" bold="true" />
		<style-class name="bracemismatch" 	fore="fdf6e3" back="dc322f" bold="true" />
		<style-class name="character" 		fore="002b36" bold="true" />
		<style-class name="class" 			fore="6c71c4" />
		<style-class name="comment" 		fore="073642" />
		<style-class name="commentbox" 		fore="073642" />
		<style-class name="commentdoc" 		fore="002b36" bold="true" />
		<style-class name="commentline" 	fore="073642" italic="true" />
		<style-class name="controlchars" 	fore="fdf6e3" />
		<style-class name="error" 			fore="fdf6e3" back="dc322f" />
		<style-class name="function" 		fore="6c71c4" />
		<style-class name="identifier" 		fore="839496" bold="true" />
		<style-class name="keyword" 		fore="002b36" bold="true" />
		<style-class name="keyword2" 		fore="6c71c4" bold="true" />
		<style-class name="linenumbers" 	fore="839496" back="eee8d5" />
		<style-class name="number" 			fore="2aa198" bold="true" />
		<style-class name="operator" 		fore="b58900" bold="true" />
		<style-class name="preprocessor" 	fore="859900" bold="true" />
		<style-class name="string" 			fore="6c71c4" bold="true" />
		<style-class name="tag" 			fore="839496" bold="true" />
		<style-class name="unclosedstring"	fore="cb4b16" back="eee8d5" />
		<style-class name="unknownattribute" fore="dc322f" bold="true" />
		<style-class name="unknowntag"		fore="dc322f" bold="true" />
<!-- Unused 
		<style-class name="whitespace"		fore="" />
		<style-class name="type"			fore="" />
-->
	</override-classes>
	<schemes>
		<scheme name="cpp">
			<override-styles>
<!-- This section is necessary because the cpp.scheme doesn't inherit correctly. -->
				<style key="11" fore="93a1a1" bold="true" />	<!--Identifiers-->
				<style key="8"  fore="2aa198" bold="true" />	<!--UUIDs-->
				<style key="17" fore="6c71c4" /> 				<!--Doc Keywords-->
				<style key="18" fore="d33682" /> 				<!--Doc Keywords Error-->
				<style key="10" fore="b58900" bold="true" /> 	<!--Operators-->
				<style key="12" fore="cb4b16" back="eee8d5" /> 	<!--Unclosed String-->
				<style key="16" fore="6c71c4" bold="true" />	<!--Keywords 2-->
				<style key="6"  fore="2aa198" bold="true" />	<!--Strings-->
				<style key="7"  fore="2aa198" bold="true" />	<!--Characters--> 
			</override-styles>
		</scheme>
	</schemes>
</UserSettings>